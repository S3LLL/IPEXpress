#!ipxe

goto lauch

:fail
	echo appuyer sur une touche pour retourner au menu principale
	prompt
	exit

:lauch
	echo Lancement de Debian
	kernel http://${shttp}/~Jean/ipxe/debian/vmlinuz1 root=/dev/nfs rw nfsroot=${snfs}:/var/lib/nfsboot/debian boot=live config quiet splash live-media-path=/live/ || goto fail
	initrd http://${shttp}/~Jean/ipxe/debian/initrd1.img || goto fail
	boot || goto fail
	echo coucou
	prompt