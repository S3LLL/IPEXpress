#!/usr/bin/env python

import os

erreure = 0

for distrib in os.listdir("mounted"):
	if distrib == "README.md":
		continue
	try:
		oups = os.system("umount mounted/" + distrib)
		if oups:
			erreure += 1
		oups = os.rmdir("mounted/" + distrib)
		if oups:
			erreure += 1
	except Exception, e:
		print(e)
		continue

if erreure!=0:
	print("doc: https://github.com/Ricain/ipexpress/wiki/umount");
	
exit(erreure)